extends ../layouts/main
include ../mixins/videoRenderer

block content
    h1 HOME
    each video in videos
        +videoRenderer(video)
    else
        div nothing found

    a(href="watch?v=32432523") button
    form(id="search-form", action="/results", method="GET")
    input(form="search-form", id="search-input", autocapitalize="none", autocomplete="off", autocorrect="off", name="search_query", type="text", spellcheck="false", placeholder="Search")